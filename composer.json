{
    "name": "sevavietl/arrays",
    "description": "This is a collection of array wrappers",
    "type": "library",
    "keywords": [
        "arrays"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "sevavietl",
            "email": "sevavietl@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "5.5.*",
        "php-coveralls/php-coveralls": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "Sevavietl\\Arrays\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sevavietl\\Arrays\\Tests\\Unit\\": "tests/unit/"
        },
        "classmap": [
            "tests/TestCase.php"
        ]
    }
}
